{
    "name": "Sci-Hub Sidebar Hider",
    "description": "Hide Sci-hub sidebar",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "https://sci-hub.se/"
    ],
    "content_scripts": [{
        "matches": ["https://sci-hub.se/*"],
        "js": ["content.js"]
    }],
    "manifest_version": 2
}